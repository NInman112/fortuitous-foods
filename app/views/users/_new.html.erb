<%= form_for user do |f| %>
	  <%= f.label :username %><br>
	  <%= f.text_field :username %><br>
	  <%= f.label :email %><br>
	  <%= f.text_field :email %><br>
	  <%= f.label :first_name%><br>
	  <%= f.text_field :first_name%><br>
	  <%= f.label :last_name %><br>
	  <%= f.text_field :last_name %><br>
	  <%= f.label :zip %><br>
	  <%= f.text_field :zip %><br>
    <%= f.label :password %><br>
    <%= f.text_field :password %><br>
	  <%= f.submit %>
	<% end %>
